{
    "name": "ananiaslitz/janus",
    "type": "library",
    "version": "0.0.1",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gateway\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Diego Ananias",
            "email": "diegohsananias@gmail.com"
        }
    ],
    "require": {
        "symfony/yaml": "^7.0",
        "guzzlehttp/guzzle": "^7.8",
        "open-telemetry/sdk": "^1.0",
        "predis/predis": "^2.2",
        "open-telemetry/opentelemetry": "^1.0",
        "php-http/httplug": "^2.4",
        "hyperf/di": "^3.1",
        "hyperf/http-server": "^3.1",
        "hyperf/utils": "^3.1",
        "hyperf/context": "^3.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "hyperf": {
            "config-provider": "Gateway\\Hyperf\\Providers\\ConfigProvider"
        }
    }
}
